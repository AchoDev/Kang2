var num = 1

loop 15 {

    var output = ""
    var print = false

    if (num % 3) == 0 {
        output = "fizz"
        print = true
    }

    if (num % 5) == 0{
        output = output + "buzz"
        print = true
    }

    if print {
        log ""
        log num
        log output
    }

    num = num + 1
}
