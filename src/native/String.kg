
struct String {
  
  get length() {
    return eval(self + ".length")
  }
  
  func split() {
    return eval(
      self + ".split()"
    )
  }

  func charAt(index) {
    return self.split()[index]
  }

  func toUpperCase() {
    return eval(self + ".toUpperCase()")
  }

